{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory-Name",
			"defaultValue": "df2bi03"
		},
		"dsBI03SynapseCustomers_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"dsBI03SynapseCustomers\""
		},
		"lsBI03Northwind_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"lsBI03Northwind\""
		},
		"lsBI03Synapse_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"lsBI03Synapse\""
		},
		"lssBI03_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"lssBI03\""
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI für Speicherkonto mit verknüpften ARM-Vorlagen"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "SAS-Token für Speicherkonto mit verknüpften ARM-Vorlagen"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df2bi03_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dsBI03SynapseCustomers_connectionString": {
						"value": "[parameters('dsBI03SynapseCustomers_connectionString')]"
					},
					"lsBI03Northwind_connectionString": {
						"value": "[parameters('lsBI03Northwind_connectionString')]"
					},
					"lsBI03Synapse_connectionString": {
						"value": "[parameters('lsBI03Synapse_connectionString')]"
					},
					"lssBI03_connectionString": {
						"value": "[parameters('lssBI03_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}